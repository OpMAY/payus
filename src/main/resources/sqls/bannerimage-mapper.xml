<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.demo.mapper.BannerImageMapper">
    <select id="getBannerImageList" resultType="BannerImage">
        select banner_no, banner_url
        from bannerimage
        order by reg_date DESC
        LIMIT 5
    </select>
</mapper>