<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mvsolutions.payus.mapper.PointWithdrawMapper">
    <insert id="requestWithdraw" keyProperty="withdraw_no" useGeneratedKeys="true">
        insert into point_withdraw (user_no, point, reg_date)
        VALUES (#{user_no}, #{request_point}, #{reg_date})
    </insert>
</mapper>