<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mvsolutions.payus.mapper.PointChargeCancelMapper">
    <insert id="insertChargeCancelRequest" keyProperty="cancel_no" useGeneratedKeys="true">
        insert into point_charge_cancel (charge_no, reason, reg_date)
        VALUES (#{charge_no}, #{reason}, #{reg_date})
    </insert>
</mapper>