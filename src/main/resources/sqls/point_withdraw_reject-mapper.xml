<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mvsolutions.payus.mapper.PointWithdrawRejectMapper">
    <select id="getWithdrawRejectReason" resultType="UserWithdrawRejectReasonResponse">
        select reject_no, reason, reg_date
        from point_withdraw_reject
        WHERE withdraw_no = #{withdraw_no}
    </select>
</mapper>